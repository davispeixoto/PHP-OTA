{
  "name": "davispeixoto/opentravel-alliance",
  "description": "A PHP implementation of OTA (OpenTravel Alliance) 2014B specifications",
  "keywords": [
    "OTA",
    "OpenTravel Alliance",
    "Open Travel Alliance"
  ],
  "repositories": [{
    "type": "vcs",
    "url": "https://github.com/goetas/serializer.git"
  }],
  "license": "MIT",
  "authors": [
    {
      "name": "Davis Peixoto",
      "email": "davis.peixoto@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "myclabs/php-enum": "1.3.2",
    "goetas/xsd2php":"2.*@dev",
    "goetas/xsd-reader":"2.*@dev",
    "jms/serializer": "xsd2php-dev as 0.18.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Davispeixoto\\OpenTravelAlliance\\": "src/Davispeixoto/OpenTravelAlliance"
    }
  },
  "minimum-stability": "dev"
}
